{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npm run build",
        "devCommand": "npm run dev"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/routes/routes.ts"
    },
    {
      "src": "/(.*)",
      "dest": "src/server.ts"
    }
  ]
}
  